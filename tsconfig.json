{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": [
      "webpack-env",
      "jest"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.tsx"
, "src/preload.js", "public/preload.ts", "src/store/modules/store/modules/global-navigation/mutations.spec.ts--fix", "src/store/modules/store/modules/local-navigation/mutations.spec.ts--fix", "src/store/modules/store/modules/local-navigation/actions.spec.ts--fix", "src/store/modules/store/index.spec.ts--fix"  ],
  "exclude": [
    "node_modules"
  ],
  
}
